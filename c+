#include <stdio.h>
#include <stdlib.h>

//This program will find the average of any numbers
//using arrays..

int main () {
int age[8];
int number;
int sum=0;
int average;

printf("Enter the total number of ages:");
scant("%d", &number);
//this is a loop
for(int n=0; n<number; ++n){
    printf("Enter the %dage:", n+1);
    scanf("%d", &age[n]);
    sum=sum+age[n];
}

average=sum/number;
printf("The average is %d", average);
   return 0;
     }
